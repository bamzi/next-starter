{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@action": "./src/action",
          "@component": "./src/component",
          "@definition": "./src/definition",
          "@interface": "./src/interface",
          "@pages": "./src/pages",
          "@store": "./src/store",
          "@styles": "./src/styles",
          "@static": "./public/static",
          "@lib": "./lib",
          "@helper": "./helper",
          "@sb": ["./storybook"]
        }
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ]
  ]
}

